@model EvernestWeb.Models.ManagerModel
@{
    ViewBag.Title = "Evernest - Manager";
}

<h2>Evernest Manager</h2>

<div>


    @if (ViewBag.Status != null)
    {
        <div class="evernest-callout evernest-callout-danger">
            <h4>Warning</h4>
            <p>
                @ViewBag.Status
            </p>
        </div>
    }


    <!-- for streams -->
    <div>
        <h4>Streams</h4>

        @{ Html.RenderPartial("_StreamTablePartial", Model.Streams); }
    </div>

    <!-- for source -->
    <div>
        <h4>Sources</h4>

        @{ Html.RenderPartial("_SourceTablePartial", Model.Sources); }
    </div>

</div>
