@model EvernestWeb.ViewModels.DeleteUserRightOnStream

@using (Html.BeginForm("DeleteUserRightOnStream", "Manager", FormMethod.Post, new {@class = "form-inline"}))
{
    @Html.AntiForgeryToken()
    <input type="hidden" name="UserId" value="@{ Write(ViewBag.UserId); }"/>
    <input type="hidden" name="StreamId" value="@{ Write(ViewBag.StreamId); }"/>
    <input type="submit" class="btn btn-default" value="Delete"/>
}